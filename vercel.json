{
    "builds": [{
        "src": "ecommerce/wsgi.py",
        "use": "@natanffs/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "build_files.sh", 
        "use": "@vercel/static-build", 
        "config": { "distDir": "staticfiles_build" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "ecommerce/wsgi.py"
        }
    ]
}